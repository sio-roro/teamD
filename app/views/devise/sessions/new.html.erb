<div class="geek-logo">
  <div class="logo"></div>
</div>
<div class="check-in">
  <div class="title">Log in</div>
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :name ,class: "name" %><br />
    <%= f.text_field :username, autofocus: true, autocomplete: "username" ,class: "input"%>
  </div>
  <br>
  <div class="field">
    <%= f.label :password ,class: "name" %><br />
    <%= f.password_field :password, autocomplete: "current-password",class: "input" %>
  </div>
  <br>
  <% if devise_mapping.rememberable? %>
  <div class="remember">
    <%= f.check_box :remember_me %>
    <%= f.label :remember_me %>
  </div>
  <% end %>
  <div class="actions">
    <%= f.submit "Log in" ,class: "submit"%>
  </div>
  <% end %>
  <%= render "devise/shared/links" %>
</div>
